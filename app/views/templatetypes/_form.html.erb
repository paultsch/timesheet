<%= form_for @templatetype do |f| %>
  <div class="form-group">
    <%= f.label :template_type %>
    <%= f.text_field :template_type, class: "form-control" %>
  </div>

  <div class="actions">
    <%= f.submit(@templatetype.new_record? ? "Create" : "Update", class:'btn btn-primary')%>
  </div>

<% end %>
